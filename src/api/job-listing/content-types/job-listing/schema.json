{
  "kind": "collectionType",
  "collectionName": "job_listings",
  "info": {
    "singularName": "job-listing",
    "pluralName": "job-listings",
    "displayName": "Job Listing"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "jobTitle": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "jobTitle"
    },
    "jobLocation": {
      "type": "string"
    },
    "jobStatus": {
      "type": "enumeration",
      "enum": [
        "Full-time",
        "Part-time",
        "Contract"
      ]
    },
    "jobDescription": {
      "type": "text"
    },
    "requirements": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean"
    }
  }
}
